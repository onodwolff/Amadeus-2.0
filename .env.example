# Copy this file to .env and adjust values for your local environment.

# Base database connection string used by SQLAlchemy.
DATABASE_URL=postgresql+asyncpg://user:pass@localhost:5432/amadeus

# Optional Redis cache connection string.
REDIS_URL=redis://localhost:6379/0

# 32-byte hex key (64 hex characters) for encrypting API secrets.
AMAD_ENC_KEY=<32-byte hex key for API secrets>
AMAD_USE_MOCK=true
